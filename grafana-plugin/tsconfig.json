{
  "extends": "@grafana/toolkit/src/config/tsconfig.plugin.json",
  "include": ["src/dummy"],
  "types": ["node", "@emotion/core"],
  "compilerOptions": {
    "rootDirs": ["src"],
    "baseUrl": "src",
    "typeRoots": ["./node_modules/@types"],
    "noUnusedLocals": false,
    "strict": false,
    "resolveJsonModule": true,
    "noImplicitAny": false,
  }
}
